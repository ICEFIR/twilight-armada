[configuration]
entry_symbol = "gdext_rust_init"
compatibility_minimum = 4.3
reloadable = true

[libraries]
linux.debug.x86_64 =     "res://../target/debug/libbetter_terrain_faster.so"
linux.release.x86_64 =   "res://../target/release/libbetter_terrain_faster.so"
linux.debug.arm64 =     "res://../target/debug/libbetter_terrain_faster.so"
linux.release.arm64 =   "res://../target/release/libbetter_terrain_faster.so"
windows.debug.x86_64 =   "res://../target/debug/better_terrain_faster.dll"
windows.release.x86_64 = "res://../target/release/better_terrain_faster.dll"
macos.debug =            "res://../target/debug/libbetter_terrain_faster.dylib"
macos.release =          "res://../target/release/libbetter_terrain_faster.dylib"
macos.debug.arm64 =      "res://../target/debug/libbetter_terrain_faster.dylib"
macos.release.arm64 =    "res://../target/release/libbetter_terrain_faster.dylib"

