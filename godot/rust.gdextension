[configuration]
entry_symbol = "gdext_rust_init"
compatibility_minimum = 4.1
reloadable = true

[libraries]
linux.debug.x86_64 =     "res://../target/debug/libtwilight_armada.so"
linux.release.x86_64 =   "res://../target/release/libtwilight_armada.so"
linux.debug.arm64 =     "res://../target/debug/libtwilight_armada.so"
linux.release.arm64 =   "res://../target/release/libtwilight_armada.so"
windows.debug.x86_64 =   "res://../target/debug/twilight_armada.dll"
windows.release.x86_64 = "res://../target/release/twilight_armada.dll"
macos.debug =            "res://../target/debug/libtwilight_armada.dylib"
macos.release =          "res://../target/release/libtwilight_armada.dylib"
macos.debug.arm64 =      "res://../target/debug/libtwilight_armada.dylib"
macos.release.arm64 =    "res://../target/release/libtwilight_armada.dylib"

